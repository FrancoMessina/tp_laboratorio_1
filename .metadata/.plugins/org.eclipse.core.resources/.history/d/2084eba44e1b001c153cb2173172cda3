int VerificarNumFactorial(float flotanteRecibido)
{
    int todoOk = 0;
    int numero;
	float resto;
	numero = flotanteRecibido;
	resto = flotanteRecibido - numero;
	if(resto == 0)
	{
		todoOk = 1;
	}
    return todoOk;
}

